<Cuisines_Category>
Chinese, FastFood, French, Italian, Japaness, Korean, Mexican, Sandwich, Thai

<Cuisine_Type>
Appetizer, Beer, Beverage, Dessert, Entree, Snacks, Wine

<Meal_Type>
Breakfast, Brunch, Lunch, HappyHour, Dinner

<Restaurant>
HASH_KEY: GeoHashPrefix
RANGE_KEY: RestaurantID
ATTRIBUTES: Name, BranchName, CustomerPrincipal(Owner), Cuisines_Category, LocalAddress, City, ZipCode, Country,
Coordinate, GeoHash, Contact, Open_Hour, Close_Hour, Images, FavoriteCounter, FollowerCounter
GSI: FavoriteCounter, FollwerCounter
GSI: City-FavoriteCounter, RestaurantID 

<Cuisine>
HASH_KEY: GeoHashPrefix 
RANGE: RestaurantID-CuisineID
ATTRIBUTES: RestaurantID, CuisineName, Cuisine_Type, Meal_Type, Price, Images, FavoriteCounter, OrderedCounter 
LSI: FavoriteCounter, OrderedCounter
GSI: Restaurant_ID

<Cuisinei_Detail>
HASH_KEY: RestaurantID
RANGE_KEY: CuisineID
ATTRIBUTES: Description, Ingredients, PriceDetail, OverallFavoriteCounter

<User_Favorite_Cuisine>
HASH_KEY: UserPrincipal
RANGE_KEY: Region_RestaurantID-CuisineID
GSI: Region-RestaurantID-CuisineID

<User_Following_Restaurant>
HASH_KEY: UserPrincipal
RANGE_KEY: Region_RestaurantID
GSI: Region_RestaurantID

